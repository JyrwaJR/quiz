{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "paths": {
      "@hooks/*": [ "./src/hooks/*" ],
      "@context/*": [ "./src/context/*" ],
      "@libs/*": [ "./src/libs/*" ],
      "@store/*": [ "./src/store/*" ],
      "@types/*": [ "./src/types/*" ],
      "@styles/*": [ "./src/styles/*" ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}
